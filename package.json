{
  "name": "rtm-cli",
  "version": "0.9.4",
  "description": "RTM CLI",
  "keywords": [
    "rtm",
    "cli",
    "terminal",
    "remember the milk",
    "tasks",
    "task manager",
    "todo",
    "gtd"
  ],
  "bin": {
    "rtm": "src/cli.js"
  },
  "main": "src/cli.js",
  "scripts": {
    "postversion": "npm run build",
    "build": "node build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dwaring87/rtm-cli.git"
  },
  "author": "David Waring <dev@davidwaring.net> (https://davidwaring.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dwaring87/rtm-cli/issues"
  },
  "homepage": "https://github.com/dwaring87/rtm-cli#readme",
  "dependencies": {
    "chalk": "^2.3.0",
    "commander": "^2.11.0",
    "dateformat": "^3.0.2",
    "deepmerge": "^2.0.1",
    "ora": "^1.3.0",
    "rtm-api": "^1.0.0"
  },
  "devDependencies": {
    "pkg": "^4.3.0-beta.1",
    "archiver": "^2.1.0"
  },
  "pkg": {
    "scripts": [
      "config.json",
      "src/cmd/*.js"
    ],
    "targets": "node8-linux-x86,node8-linux-x64,node8-macos-x86,node8-macos-x64,node8-win-x86,node8-win-x64",
    "out": "./bin"
  }
}
